{
  "name": "express-skeleton",
  "version": "1.0.0",
  "description": "Express JS MVC project skeleton",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "model:generate": "sequelize model:generate",
    "migrate": "sequelize db:migrate",
    "migrate:rollback": "sequelize db:migrate:undo",
    "seed:generate": "sequelize seed:generate",
    "seed": "sequelize db:seed:all",
    "seed:name": "sequelize db:seed --seed",
    "seed:rollback": "sequelize db:seed:undo:all",
    "seed:rollback:name": "sequelize db:seed:undo --seed"
  },
  "author": "Adi Purnama",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mariadb": "^2.3.1",
    "moment": "^2.29.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "uuidv4": "^6.2.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  },
  "engines": {
    "node": "12.*",
    "npm": "6.*"
  }
}
